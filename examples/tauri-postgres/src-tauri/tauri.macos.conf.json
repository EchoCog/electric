{
    "$schema": "../node_modules/@tauri-apps/cli/schema.json",
    "tauri": {
      "allowlist": {
        "all": false,
        "shell":{
          "scope": [
            { "name": "./ollama-darwin", "sidecar": true }
          ],
          "sidecar": true
        }
      },
      "bundle": {
        "externalBin": [
          "./ollama-darwin"
        ],
        "identifier": "electric-tauri-postgres",
        "macOS": {
          "entitlements": null,
          "exceptionDomain": "",
          "frameworks": [],
          "providerShortName": null,
          "signingIdentity": null
        },
        "resources": ["./libonnxruntime.dylib", "./pgdir/*"]
      }
    }
  }
